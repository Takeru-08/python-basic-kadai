        "import numpy as np\n",
        "\n",
        "#多次元配列のコードを記述\n",
        "A = np.arange(6).reshape(3,2)\n",
        "B = np.arange(8).reshape(2,4)\n",
        "\n",
        "#行列の積を計算\n",
        "total = np.dot(A,B)\n",
        "\n",
        "#計算結果を記述\n",
        "print(total)\n",
        "\n",
        "#最大値を求める\n",
        "print(f\"最大値:{total.max()}\")"
      ],
